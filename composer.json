    "homepage": "http://golem.yoozi.cn/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saturn HU",
            "email": "yangg.hu@yoozi.cn",
            "homepage": "http://www.yoozi.cn/"
        }
    ],
    "require": {
        "php": ">=5.3.0",
